{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/tmp/a.log",
            "log_group_name": "docker_stream1",
            "log_stream_name": "docker_stream1",
            "retention_in_days": 365
          }
        ]
      }
    }
  }
}